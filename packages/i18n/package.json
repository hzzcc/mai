{
  "name": "@mai/i18n",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "module": "./esm/index.js",
  "main": "./cjs/index.js",
  "types": "./esm/index.d.ts",
  "files": [
    "esm",
    "cjs"
  ],
  "scripts": {
    "dev": "rslib build --watch",
    "build": "rslib build"
  },
  "dependencies": {
    "i18next": "^25.6.1",
    "react-i18next": "^16.2.4"
  },
  "peerDependencies": {
    "@types/react": ">=19.2.0",
    "@types/react-dom": ">=19.2.0",
    "react": ">=19.2.0",
    "react-dom": ">=19.2.0"
  },
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "type": "./esm/index.d.ts",
      "require": "./cjs/index.js"
    },
    "./*": {
      "import": "./esm/*/index.js",
      "type": "./esm/*/index.d.ts",
      "require": "./cjs/*/index.js"
    }
  }
}
