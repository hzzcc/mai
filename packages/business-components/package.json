{
  "name": "@mai/business-components",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "module": "./esm/index.js",
  "main": "./cjs/index.js",
  "types": "./esm/index.d.ts",
  "files": [
    "esm",
    "cjs"
  ],
  "sideEffects": [
    "**/*.scss"
  ],
  "scripts": {
    "dev": "rslib build --watch",
    "build": "rslib build"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "lodash": "^4.17.21",
    "@mai/components": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0",
    "@types/react": ">=16.9.0",
    "@types/react-dom": ">=16.9.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "react-router-dom": "^6.14.1"
  },
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "type": "./esm/index.d.ts",
      "require": "./cjs/index.js"
    },
    "./*": {
      "import": "./esm/*/index.js",
      "type": "./esm/*/index.d.ts",
      "require": "./cjs/*/index.js"
    }
  }
}
